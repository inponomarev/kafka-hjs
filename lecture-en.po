#: lecture.adoc:2658
msgid ""
"[source,sql]\n"
"----\n"
"CREATE STREAM pageviews_enriched AS\n"
"  SELECT pv.viewtime,\n"
"         pv.userid AS userid,\n"
"         pv.pageid,\n"
"         pv.timestring,\n"
"         u.gender,\n"
"         u.regionid,\n"
"         u.interests,\n"
"         u.contactinfo\n"
"  FROM pageviews_transformed pv\n"
"  LEFT JOIN users_5part u ON pv.userid = u.userid\n"
"  EMIT CHANGES;\n"
"----"
msgstr ""
"[source,sql]\n"
"----\n"
"CREATE STREAM pageviews_enriched AS\n"
"  SELECT pv.viewtime,\n"
"         pv.userid AS userid,\n"
"         pv.pageid,\n"
"         pv.timestring,\n"
"         u.gender,\n"
"         u.regionid,\n"
"         u.interests,\n"
"         u.contactinfo\n"
"  FROM pageviews_transformed pv\n"
"  LEFT JOIN users_5part u ON pv.userid = u.userid\n"
"  EMIT CHANGES;\n"
"----"

#: lecture.adoc:2660
msgid "== KSQL"
msgstr "== KSQL"

#: lecture.adoc:2673
msgid ""
"[source,sql]\n"
"----\n"
"CREATE TABLE pageviews_per_region_per_30secs AS\n"
"  SELECT regionid,\n"
"         count(*)\n"
"  FROM pageviews_enriched\n"
"  WINDOW TUMBLING (SIZE 30 SECONDS)\n"
"  WHERE UCASE(gender)='FEMALE' AND LCASE(regionid)='region_6'\n"
"  GROUP BY regionid\n"
"  EMIT CHANGES;\n"
"----"
msgstr ""
"[source,sql]\n"
"----\n"
"CREATE TABLE pageviews_per_region_per_30secs AS\n"
